<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Harga Ongkir List
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">

                <div class="panel panel-default">
                    <div class="panel-heading">
                        Filter
                        <div class="btn-group pull-right">
                            <a href="#">
                                <span class="glyphicon glyphicon-search" onclick=""></span>
                            </a>
                        </div>
                    </div>
                    <div class="panel-body">
                        <div role="form">
                            <div class="form-group">
                                <label>Provinsi Asal</label>
                                @{
                                    var listsortProvinsi = new List<SelectListItem>();
                                    for (int i = 0; i < 2; i++)
                                    {
                                        listsortProvinsi.Add(new SelectListItem
                                        {
                                            Value = "A",
                                            Text = "A"
                                        });
                                    }
                                }
                                @Html.DropDownList("SortBy", listsortProvinsi, new { @class = "form-control", @onchange = "CallChangefunc(this.value)", style = "width:300px" })
                            </div>
                            <div class="form-group">
                                <label>Kabupaten Asal</label>
                                @{
                                    var listsortKabupaten = new List<SelectListItem>();
