                                @{
                                    var listsortKecamatanTujuan = new List<SelectListItem>();
                                    for (int i = 0; i < 2; i++)
                                    {
                                        listsortKecamatanTujuan.Add(new SelectListItem
                                        {
                                            Value = "A",
                                            Text = "A"
                                        });
                                    }
                                }
                                @Html.DropDownList("SortBy", listsortKecamatanTujuan, new { @class = "form-control", @onchange = "CallChangefunc(this.value)", style = "width:300px" })
                            </div>
                            <div class="form-group">
                                <label>Pengirim</label>
                                @{
                                    var listsortPengirim = new List<SelectListItem>();
                                    for (int i = 0; i < 2; i++)
