                            </div>
                            <div class="form-group">
                                <label>Paket</label>
                                @{
                                    var listsortPaket = new List<SelectListItem>();
                                    for (int i = 0; i < 2; i++)
                                    {
                                        listsortPaket.Add(new SelectListItem
                                        {
                                            Value = "A",
                                            Text = "A"
                                        });
                                    }
                                }
                                @Html.DropDownList("SortBy", listsortPaket, new { @class = "form-control", @onchange = "CallChangefunc(this.value)", style = "width:300px" })
                            </div>
                        </div>

                    </div>
                </div>
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <th style="width:50px"></th>
                            <th style="width:50px"></th>
                            <th>Provinsi Asal</th>
                            <th>Kabupaten Asal</th>
                            <th>Kecamatan Asal</th>
                            <th>Provinsi Tujuan</th>
                            <th>Kabupaten Tujuan</th>
                            <th>Kecamatan Tujuan</th>
                            <th>Harga</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (int i = 0; i < 2; i++)
                        {
                            <tr class="odd gradeX">
                                <td>
                                    <input type="button" name="name" value="Edit" class="btn btn-primary btn-sm" onclick=")" />
                                </td>
                                <td>
                                    <input type="button" name="name" value="Delete" class="btn btn-danger btn-sm" onclick=")" />
                                </td>
                                <td>-</td>
                                <td>-</td>
                                <td>-</td>
                                <td>-</td>
                                <td>-</td>
                                <td>-</td>
                                <td>-</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>