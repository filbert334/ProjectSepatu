            <div class="panel-body">

                <div class="panel panel-default">
                    <div class="panel-heading">
                        Filter
                        <div class="btn-group pull-right">
                            <a href="#">
                                <span class="glyphicon glyphicon-search" onclick=""></span>
                            </a>
                        </div>
                    </div>
                    <div class="panel-body">
                        <form role="form" id="filterBrandList">
                            <div class="form-group">
                                <label>Provinsi</label>
                                @{
                                    var listsortProvinsi = new List<SelectListItem>();
                                    listsortProvinsi.Add(new SelectListItem
                                    {
                                        Value = "A",
                                        Text = "A"
                                    });
                                    listsortProvinsi.Add(new SelectListItem
                                    {
                                        Value = "B",
                                        Text = "B"
                                    });
                                    listsortProvinsi.Add(new SelectListItem
                                    {
                                        Value = "B",
                                        Text = "B"
                                    });
                                    listsortProvinsi.Add(new SelectListItem
                                    {
                                        Value = "C",
                                        Text = "C"
                                    });
                                }
                                @Html.DropDownList("SortBy", listsortProvinsi, new { @class = "form-control", @onchange = "CallChangefunc(this.value)", style = "width:300px" })
                            </div>
                            <div class="form-group">
                                <label>Kecamatan</label>
                                @{
                                    var listsortKecamatan = new List<SelectListItem>();
                                    listsortKecamatan.Add(new SelectListItem
                                    {
                                        Value = "A",
