
@{
    ViewData["Title"] = "TabMetodePembayaran";
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Metode Pembayaran
                <div class="btn-group pull-right">
                    <a href="#">
                        <span class="glyphicon glyphicon-search" onclick=""></span>
                    </a>
                    <a href="#">
                        <span class="glyphicon glyphicon-file" onclick=""></span>
                    </a>
                    <a href="#">
                        <span class="glyphicon glyphicon-floppy-disk" onclick=""></span>
                    </a>
                </div>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <form role="form" id="filterBrandList">
                    <div class="form-group">
                        <label>Metode Pembayaran</label>
                        @Html.TextBox("BrandName", "", new { @class = "form-control", style = "width:300px" })
                    </div>
                </form>
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <th style="width:50px"></th>
                            <th style="width:50px"></th>
                            <th>Metode Pembayaran</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (int i = 0; i < 2; i++)
                        {
                            <tr class="odd gradeX">
                                <td>
                                    <input type="button" name="name" value="Edit" class="btn btn-primary btn-sm" onclick=")" />
                                </td>
                                <td>
                                    <input type="button" name="name" value="Delete" class="btn btn-danger btn-sm" onclick=")" />
                                </td>
                                <td>Metode</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

